<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<Version>1.0.3</Version>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<Authors>Aleksandar Radic</Authors>
	<PackageId>ImplementationScanner</PackageId>
	<Description>Generates fake events and serializes them as JSON</Description>
	<PackageTags>events;faker;generator;class scaner</PackageTags>
	<RepositoryUrl>https://github.com/Rad1c/ImplementationScanner</RepositoryUrl>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Bogus" Version="35.6.3" />
  </ItemGroup>

  <ItemGroup>
	  <None Update="README.md">
		  <Pack>True</Pack>
		  <PackagePath>\</PackagePath>
	  </None>
  </ItemGroup>

</Project>
